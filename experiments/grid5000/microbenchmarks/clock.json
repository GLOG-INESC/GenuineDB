{
  "ycsb-zipf": {
    "servers": [
      {
        "config": "grid5000/microbenchmarks/glog.conf",
        "image": "jrafaelsoares/geozip_image",
        "binary": "global_log",
        "build_dir": "glog",
        "system": "GLOG",
        "replication": "PARTIAL",
        "execution": "PARTIAL",
        "server_reset": true
      }
    ],
    "workload": "ycsb",
    "txn_issuing_delay" : [0],
    "jitter": [1],
    "jitter_constant": true,
    "duration": 70,
    "clients": [64],
    "generators": [16],
    "zipf": [0.75],
    "mh": [5],
    "mix": ["50_0_50"],
    "fi" : [0]
  }
}