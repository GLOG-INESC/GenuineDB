{
  "ycsb-zipf": {
    "servers": [
      {
        "config": "grid5000/microbenchmarks/glog.conf",
        "image": "jrafaelsoares/geozip_image",
        "binary": "global_log",
        "build_dir": "glog",
        "system": "GLOG",
        "replication": "PARTIAL",
        "execution": "PARTIAL",
        "server_reset": true
      },
      {
        "config": "grid5000/microbenchmarks/slog.conf",
        "image": "jrafaelsoares/geozip_image",
        "binary": "slog",
        "build_dir": "slog",
        "system": "SLOG",
        "replication": "PARTIAL",
        "execution": "PARTIAL",
        "server_reset": true
      },
      {
        "config": "grid5000/microbenchmarks/detock_pr.conf",
        "image": "jrafaelsoares/geozip_image",
        "binary": "slog",
        "build_dir": "detock",
        "system": "DETOCK",
        "replication": "PARTIAL",
        "execution": "PARTIAL",
        "server_reset": true
      }
    ],
    "workload": "ycsb",
    "txn_issuing_delay" : [0],
    "jitter": [0, 0.05, 0.10, 0.25, 0.50, 0.75],
    "duration": 70,
    "clients": [512],
    "generators": [16],
    "zipf": [1.0],
    "mh": [10],
    "mix": ["50_0_50"],
    "fi" : [0]
  }
}